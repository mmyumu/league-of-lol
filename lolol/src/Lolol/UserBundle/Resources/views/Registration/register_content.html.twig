{% trans_default_domain 'FOSUserBundle' %}

{% form_theme form with 'LololUserBundle:Form:fields.html.twig' %}

<div class="row">
	<div class="col-xs-8 col-sm-6 col-md-5">
		<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register" role="form">
		    {{ form_widget(form) }}
		    <div>
		        <input type="submit" value="{{ 'registration.submit'|trans }}" class="btn btn-primary" />
		    </div>
		</form>
	</div>
</div>
